class Solution:
    def checkEqual(self, a, b) -> bool:
        #code here
        freq={}
        for i in a:
            if i in freq.keys():
                freq[i]+=1
            else:
                freq[i]=1
        for i in b:
            if i not in freq.keys():
                return False
            freq[i]-=1
            if freq[i]<0:
                return False
        return True
    
            
